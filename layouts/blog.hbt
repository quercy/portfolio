
{{> header}}
	<div id="content" class="blog">
	{{{ contents }}}

	{{#each collections.posts}}
		<div class="item">
            <h4><a href="{{{chopString this.path 1}}}">{{this.title}}</a></h4>
            {{{date this.date}}}
            <article>{{this.tagline}}</article>
        </div>
	{{/each}}
	</div>

{{> footer}}

